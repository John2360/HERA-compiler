#include "Tiger-stdlib-stack-data.hera"

DLABEL(my_string_1) 
    LP_STRING("D'oh!\012") 

DLABEL(my_string_2) 
    LP_STRING("If this is the only output, the 'while false' test seems to work\012") 

CBON()

LABEL(my_while_cond_1)
    SET(R1, 0)

CMP(R1, R0)
BZ(my_while_post_1)
MOVE(FP_alt, SP)
INC(SP, 4)
SET(R1, my_string_1)
STORE(R1, 3, FP_alt) 
CALL(FP_alt, print)
LOAD(R2, 3, FP_alt)
DEC(SP, 3) 


BR(my_while_cond_1)
LABEL(my_while_post_1)
MOVE(FP_alt, SP)
INC(SP, 4)
SET(R1, my_string_2)
STORE(R1, 3, FP_alt) 
CALL(FP_alt, print)
LOAD(R2, 3, FP_alt)
DEC(SP, 3) 

MOVE(R2, R2)


HALT()
#include "Tiger-stdlib-stack.hera"
